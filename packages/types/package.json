{
  "name": "@educational-app/types",
  "version": "0.1.0",
  "description": "Shared TypeScript type definitions",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.0"
  },
  "files": [
    "dist"
  ]
}