{
  "name": "@educational-app/media-manager",
  "version": "0.1.0",
  "description": "Reusable image upload and management system for Educational App platform",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "svelte": "./dist/index.js"
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "dependencies": {
    "@vercel/blob": "^0.23.4",
    "zod": "^3.23.8",
    "@educational-app/i18n": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.4",
    "prettier": "^3.3.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.4",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "svelte": "./dist/index.js",
  "keywords": [
    "svelte",
    "image-upload",
    "vercel-blob",
    "media-manager"
  ]
}